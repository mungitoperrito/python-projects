# tuples


d = {'a': 1, 'b': 2, 'g': 7}
print(type(d))

for k,v in d.items():
    # NOTE: k,v aren't a tuple 
    print(f"k: {k} v: {v} types: {type(k), type(v)} ")

for di in d:
    # NOTE: di is a string
    print(f"di: {di} type: {type(di)} ")

for di in d.items():
    # NOTE: di is a tuple
    print(f"di: {di} type: {type(di)} ")
